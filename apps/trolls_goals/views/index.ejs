<% include ../../../views/head_start %>
  <link rel="stylesheet" href="/css/bootstrap.min.css" />
  <link rel="stylesheet" href="/css/select.css" />
<% include ../../../views/head_end_and_header %>
  <div ng-app="app" id="wrapper">
    <div id="sidebar-wrapper">
      <a href="#/domains">
        <img src="/logo_150.jpg" />
      </a>
      <ul class="sidebar-nav">
        <!--<navlink href="domains" title="All Areas"></navlink>
        <navlink ng-repeat="domain in domains" href="domains/{{domain.name}}" title="{{domain.name}}"></navlink>-->
        <navlink href="day" title="Today"></navlink>
        <navlink href="week" title="This Week"></navlink>
      </ul>
      <button ng-show="! show_hidden" class="btn btn-primary btn-xs"
              ng-init="show_hidden = false"
              ng-click="show_hidden = true" title="Show hidden domains and areas">
        Show Hidden
        <span class="glyphicon glyphicon-eye-open"></span>
      </button>
      <button ng-show="show_hidden" class="btn btn-primary btn-xs"
              ng-click="show_hidden = false" title="Hide hidden domains and areas">
        Hide Hidden
        <span class="glyphicon glyphicon-eye-close"></span>
      </button>
    </div>
    <div id="page-content-wrapper">
      <div class="page-content">
        <div ng-view>
        </div>
      </div>
    </div>
  </div>
