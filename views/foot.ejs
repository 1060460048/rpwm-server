  <!-- JavaScript at the bottom for fast page loading -->
  <!-- Grab Google CDN's jQuery, with a protocol relative URL; fall back to local if offline -->
  <script>window.jQuery || document.write('<script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"><\/script>')</script>
  <script>window.jQuery || document.write('<script src="http://<%= settings.base_url %>/js/lib/jquery-1.7.1.min.js"><\/script>')</script>

  <!-- Navbar script (for dropdown animation) -->
  <% if (! settings.hide_navbar) { %>
    <script src="http://<%= settings.base_url %>/js/lib/jquery.hoverIntent.min.js"></script>

    <script>
      $('.navbar li ul').css({
        display: 'none',
        left: 'auto'
      });

      $('.navbar li').hoverIntent(function() {
        $(this)
          .find('ul')
          .stop(true, true)
          .slideDown('fast');

      }, function() {
        $(this)
          .find('ul')
          .stop(true, true)
          .fadeOut('fast');
      });
    </script>
  <% } %>


  <% if (settings.show_banner) { %> 
  <!-- fade in the "under construction" message -->
    <script>$('#under_construction').hide().fadeIn(2000);</script> 
  <% } %>

  <!-- scripts concatenated and minified via build script -->
  
  <script src="js/plugins.js"></script>
  <script src="js/script.js"></script>
